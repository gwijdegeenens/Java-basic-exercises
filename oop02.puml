@startuml
'https://plantuml.com/class-diagram


class Rectangle {
	+ int width
	+ int height
	+ int x
	+ int y
		+ void setX(int x)
       + void setY(int y)
    	+ void setWidth(int width)
    	+ void setHeight(int height)
    	+ void setPosition(int x, int y)
    	+ void grow(int d)
}

@enduml